        <script type="text/javascript">
  function redirectToNew() {
    var klass = $("new_object_type").value;
  
    if (klass == "Workflow")
      location.href = "/workflows/new";
  
    if (klass == "File")
      location.href = "/files/new";
  
    if (klass == "Pack")
      location.href = "/packs/new";
  
    if (klass == "Group")
      location.href = "/groups/new";
  
    if (klass == "Message")
      location.href = "/messages/new";
  
    if (klass == "Runner")
      location.href = "/runners/new";
  
    if (klass == "Experiment")
      location.href = "/experiments/new";
  
    if (klass == "Job")
      location.href = "/jobs/new";
  
  }
</script>


<div id="new_object">
  <form onsubmit="redirectToNew(); return false;">
    <select id="new_object_type">
      
        
        <option value="Workflow" >New Workflow</option>
      
        
        <option value="File" >New File</option>
      
        
        <option value="Pack" >New Pack</option>
      
        
        <option value="Group" >New Group</option>
      
        
        
        
        
