	public void efetuaRecomendacao(String[][] matrizCSV) {
		
		//System.out.println(matrizCSV.length);
		String [] headers = new String[281];
		String [] ontologias = new String[73];
		String [] wfs = new String[73];
		
		for (int i = 0; i < matrizCSV[0].length; i++) {//Pega headers
		//	System.out.println((i)+"-"+matrizCSV[0][i]);
			headers[i] = matrizCSV[0][i];
		}
		//System.out.println("--------------------------------------------------------------");
		for (int i = 0; i < matrizCSV.length; i++) {//Pega Ontologias
		//	System.out.println((i)+"-"+matrizCSV[i][280]);
			ontologias[i] = matrizCSV[i][280];
		}
		
		for (int i = 0; i < matrizCSV[0].length; i++) {//Pega Colunas
		//	System.out.println((i+1)+"-"+matrizCSV[i][0]);
			//wfs[i] = matrizCSV[0][i];
			//for (int j = 0; j < matrizCSV.length; j++) {//Pega Linhas
			
			for (int j = 0; j < (matrizCSV.length); j++) {//Pega Linhas uso j+1 para acessar a ontologia do trem
				//System.out.println("valor de j "+j);
				//System.out.print(matrizCSV[j][i]);
				
				if(matrizCSV[j][i].equals("1")){
					System.out.println(j);
					//if((j+1) < 73)
						System.out.println("valor de j = "+j+" Ontologia = "+ontologias[j]);
				}
			}
			//System.out.println();
		}
		
		
		//String [] wfs = new String[72];
		//String [] headers = new String[281];
		
		//ArrayList<ArrayList<String>> arr = new ArrayList<ArrayList<String>>();
		//String[]array = new String[73];
		
		
		//System.out.println("Done!");
	}
	

