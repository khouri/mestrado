package Main;

import java.io.BufferedReader;
import java.io.FileNotFoundException;
import java.io.FileReader;
import java.io.IOException;


public class CriaListaAnotacoes {

	public static void main(String[] args) {

		CriaListaAnotacoes obj = new CriaListaAnotacoes();
		obj.run();
	}
	
	public void imprimeArray(String[] imprimir){
	
		for (int i = 0; i < imprimir.length; i++)
			System.out.print(imprimir[i]);
		System.out.println();
	}

	public void run() {

		String csvFile = "/home/toasty/Desktop/DADOSFIN.csv";
		BufferedReader br = null;
		String line = "";
		String cvsSplitBy = ",";

		try {

			br = new BufferedReader(new FileReader(csvFile));
			while ((line = br.readLine()) != null) {

				// use comma as separator
				String[] workflowCSVLido = line.split(cvsSplitBy);
				//Laco para percorrer a linha inteira
				imprimeArray(workflowCSVLido);
			}

		} catch (FileNotFoundException e) {
			e.printStackTrace();
		} catch (IOException e) {
			e.printStackTrace();
		} finally {
			if (br != null) {
				try {
					br.close();
				} catch (IOException e) {
					e.printStackTrace();
				}
			}
		}

		System.out.println("Done");
	}
}