<div class="default_timeline" id="timeline_73510800"></div><script type="text/javascript">
//<![CDATA[
var timeline_73510800;var timeline_73510800_timer=null;
var timeline_73510800_event_source=new Timeline.DefaultEventSource();
Event.observe(window,'load',function(){timeline_73510800_onLoad()});
Event.observe(window,'resize',function(){timeline_73510800_onResize()});
function timeline_73510800_onLoad(){

var band_73510700=Timeline.createBandInfo({
trackGap:0.1,showEventText:false,width:"25%",intervalUnit:Timeline.DateTime.MONTH,intervalPixels:200,eventSource:timeline_73510800_event_source
});
var band_73510120=Timeline.createBandInfo({
trackGap:0.1,showEventText:false,width:"25%",intervalUnit:Timeline.DateTime.DAY,intervalPixels:100,eventSource:timeline_73510800_event_source
});
var band_73509540=Timeline.createBandInfo({
width:"50%",intervalUnit:Timeline.DateTime.HOUR,intervalPixels:20,eventSource:timeline_73510800_event_source
});
timeline_73510800_bands=[band_73510700,band_73510120,band_73509540];
timeline_73510800_bands[0].syncWith=2;
timeline_73510800_bands[1].syncWith=2;
timeline_73510800_bands[0].highlight=true;
timeline_73510800_bands[1].highlight=true;
timeline_73510800=Timeline.create($('timeline_73510800'),timeline_73510800_bands);
timeline_73510800.getBand(1).addOnScrollListener(function(timeline){
if (timeline_73510800_timer==null){
timeline_73510800_timer=window.setTimeout(function(){
timeline_73510800_timer=null;
var start_date1=timeline.getMinDate();var start_date2=start_date1;
var end_date1=timeline.getMaxDate();var end_date2=end_date1;
var earliest_event=timeline.getEventSource().getEarliestDate();
var latest_event=timeline.getEventSource().getLatestDate();
if(start_date2<latest_event){start_date2=latest_event}
if(end_date1>earliest_event){end_date1=earliest_event}
if(end_date1>start_date1){
Timeline.loadJSON(
event_source_url("http://www.myexperiment.org/workflows/235/comments.json?start=REPL_START&amp;end=REPL_END",start_date1,end_date1),
function(xml,url){timeline_73510800_event_source.loadJSON(xml,url);})}
if(end_date2>start_date2){
Timeline.loadJSON(
event_source_url("http://www.myexperiment.org/workflows/235/comments.json?start=REPL_START&amp;end=REPL_END",start_date2,end_date2),
function(xml,url){timeline_73510800_event_source.loadJSON(xml,url);})
}}
, 500)}});
Timeline.loadJSON(event_source_url("http://www.myexperiment.org/workflows/235/comments.json?start=REPL_START&amp;end=REPL_END",timeline_73510800.getBand(1).getMinDate(),timeline_73510800.getBand(1).getMaxDate()),function(xml,url){timeline_73510800_event_source.loadJSON(xml,url);});
};
function timeline_73510800_onResize(){if (timeline_73510800_timer==null){timeline_73510800_timer=window.setTimeout(function(){timeline_73510800_timer=null;timeline_73510800.layout();timeline_73510800.paint();},500);}};
function event_source_url(base_url,start,end){
if (start==null){start = new Date();}
if (end==null){end = new Date();}
return base_url.replace('REPL_START', escape(start.toUTCString())).replace('REPL_END', escape(end.toUTCString()));
}
//]]>
</script>

