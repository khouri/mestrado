						    <td>GetUniqueHomolog:MouseGeneIDs</td>
						  </tr>
						
						  <tr>
						    <td>hsapiens_gene_ensembl:hsapiens_gene_ensembl.rat_ensembl_gene</td>
						    <td>GetUniqueHomolog:RatGeneIDs</td>
						  </tr>
						
						  <tr>
						    <td>plot:Graphics_in_PNG</td>
						    <td>FlattenImageList:inputlist</td>
						  </tr>
						
						  <tr>
						    <td>seqret:outseq</td>
						    <td>emma:sequence_direct_data</td>
						  </tr>
						
					</table>
				
			</div>
		</div>
		
		<!-- Coordinations -->
		
		<div class="fold">
			<div class="foldTitle">
		    Coordinations (0)
			</div>
		  <div class="foldContent" style="display: none;">
				
					<p class="none_text">None</p>
				
			</div>
		</div>
	
	


        
			
		</div>
	
	</div>
	
