			<h3>
				<img alt="Information" src="/images/famfamfam_silk/information.png?1202402239" style="vertical-align:middle;" title="header=[] body=[This section shows the internal components of this workflow version] cssheader=[boxoverTooltipHeader] cssbody=[boxoverTooltipBody] delay=[200]" />
				Workflow Components
			</h3>
			
			
        
          

	
		
		<!-- Sources -->
		
		<div class="fold">
			<div class="foldTitle">
		    Inputs (0)
			</div>
		  <div class="foldContent" style="display: none;">
				
					<p class="none_text">None</p>
				
			</div>
		</div>
		
		<!-- Processors -->
		
		<div class="fold">
			<div class="foldTitle">
		    Processors (7)
			</div>
		  <div class="foldContent" style="display: none;">
				
					<table class="simple">
					  <tr>
					    <th>Name</th>
							<th>Type</th>
					    <th>Description</th>
					  </tr>
					
						
						  <tr>
						    <td><b>Ask_for_sequence</b></td>
								<td>local</td>
						    <td>Ask for the input sequence (fasta format recommended) or a sequence Id in &quot;database:id&quot; format (e.g. uniprot:wap_rat).</td>
						  </tr>
						
						  <tr>
						    <td><b>Make_database_list</b></td>
								<td>local</td>
						    <td>A list of database names to use in a select.</td>
						  </tr>
						
						  <tr>
						    <td><b>Select_program</b></td>
								<td>local</td>
						    <td>Select the BLAST program to use for the search.</td>
						  </tr>
						
						  <tr>
						    <td><b>Select_database</b></td>
								<td>local</td>
						    <td>Select the database to search.</td>
						  </tr>
						
						  <tr>
						    <td><b>Ask_for_email</b></td>
								<td>local</td>
						    <td>Ask for the user&#39;s e-mail address.</td>
						  </tr>
						
						  <tr>
						    <td><b>Make_program_list</b></td>
								<td>local</td>
						    <td>A liast or program names to use in the select.</td>
						  </tr>
						
						  <tr>
						    <td><b>EBI_WU_BLAST</b></td>
								<td>workflow</td>
						    <td>Run a BLAST analysis using the EBI&#39;s WU-BLAST service (see http://www.ebi.ac.uk/Tools/webservices/services/wublast).</td>
						  </tr>
						
					</table>
				
			</div>
		</div>
		
		<!-- Beanshells -->
    
